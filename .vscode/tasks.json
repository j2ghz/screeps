{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "screeps check",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo screeps check"
		},
		{
			"type": "cargo",
			"command": "screeps upload",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "rust: cargo screeps upload"
		}
	]
}